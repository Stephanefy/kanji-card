{
  "name": "kanji-card",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@elastic/eui": "^26.1.0",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@splidejs/react-splide": "^0.7.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "autoprefixer": "^9.8.0",
    "firebase": "^7.14.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-card-flip": "^1.0.10",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-parallax-tilt": "^1.3.42",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-window": "^1.8.6",
    "tailwindcss": "^1.4.6"
  },
  "scripts": {
    "build:css": "postcss src/styles/App.css -o src/index.css",
    "watch:css": "postcss src/styles/App.css -o src/index.css -w",
